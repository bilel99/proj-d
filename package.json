{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-generatornewpassword": "cd nova-components/Generatornewpassword && npm run dev",
        "build-generatornewpassword-prod": "cd nova-components/Generatornewpassword && npm run prod",
        "build-contactcard": "cd nova-components/Contactcard && npm run dev",
        "build-contactcard-prod": "cd nova-components/Contactcard && npm run prod",
        "build-doctorinfocard": "cd nova-components/Doctorinfocard && npm run dev",
        "build-doctorinfocard-prod": "cd nova-components/Doctorinfocard && npm run prod",
        "build-doctorsinfoscard": "cd nova-components/Doctorsinfoscard && npm run dev",
        "build-doctorsinfoscard-prod": "cd nova-components/Doctorsinfoscard && npm run prod"
    },
    "devDependencies": {
        "axios": "^0.19",
        "bootstrap": "^4.4.1",
        "cross-env": "^7.0",
        "jquery": "^3.5.1",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.13",
        "popper.js": "^1.12",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "vue": "^2.5.17",
        "vuex": "^3.4.0",
        "vue-router": "^3.3.2",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "animate.css": "^4.1.0",
        "luxon": "^1.28.1",
        "sweetalert2": "^9.15.1",
        "vue-datetime": "^1.0.0-beta.13",
        "vue-multiselect": "^2.1.6",
        "weekstart": "^1.0.1"
    }
